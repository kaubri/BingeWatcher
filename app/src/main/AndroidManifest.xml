<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mikaila.otakubinge">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_otakubinge_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_otakubinge_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mikaila.otakubinge.SearchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mikaila.otakubinge.ResultActivity"
            android:parentActivityName="com.mikaila.otakubinge.SearchActivity">
        </activity>
    </application>

</manifest>